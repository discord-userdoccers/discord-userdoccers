---
import type { HTMLAttributes } from "astro/types";
interface Props extends HTMLAttributes<"svg"> {}
---

<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 450.58 233.56" class="dark:text-text-dark text-text-light fill-current" {...Astro.props}>
	<text class="snowflake-dec-number snowflake-dec-sz-normal snowflake-dec-reduce-space" transform="translate(164.03 9.96)"> 175928847299117063 </text>
	<text class="snowflake-dec-text snowflake-dec-sz-small" transform="translate(233.5 39.22)"> to binary </text>
	<text class="snowflake-dec-text snowflake-dec-sz-small snowflake-dec-ms" transform="translate(29.68 81.54)">
		Number of milliseconds since the Discord epoch (first second of 2015)
	</text>
	<text class="snowflake-dec-text snowflake-dec-sz-small snowflake-dec-worker" transform="translate(298.11 81.17)">
		internal
		<tspan x="1.12" y="9.6"> worker </tspan>
		<tspan x="9.12" y="19.2"> ID </tspan>
	</text>
	<text class="snowflake-dec-text snowflake-dec-sz-small snowflake-dec-process" transform="translate(335.06 81.17)">
		internal
		<tspan x="-0.89" y="9.6"> process </tspan>
		<tspan x="9.12" y="19.2"> ID </tspan>
	</text>
	<text class="snowflake-dec-text snowflake-dec-sz-small snowflake-dec-increment" transform="translate(369.42 81.17)">
		incremented for every
		<tspan x="2.21" y="9.6"> generated ID on that </tspan>
		<tspan x="24.68" y="19.2"> process </tspan>
	</text>
	<text class="snowflake-dec-number snowflake-dec-sz-normal snowflake-dec-reduce-space cls-7" transform="translate(3.75 70.32)">
		<tspan class="snowflake-dec-ms">000000100111000100000110010110101100000100</tspan>
		<tspan class="snowflake-dec-worker" x="292.82" y="0"> 00001 </tspan>
		<tspan class="snowflake-dec-process" x="327.68" y="0"> 00000 </tspan>
		<tspan class="snowflake-dec-increment" x="362.54" y="0"> 000000000111 </tspan>
	</text>
	<g class="snowflake-dec-arrow">
		<rect x="226.41" y="21.42" width="0.53" height="25.39"></rect>
		<polygon points="226.68 45.37 224.42 44.36 226.67 48.81 228.91 44.36 226.68 45.37"></polygon>
	</g>
	<g class="snowflake-dec-arrow">
		<rect x="89.45" y="83.72" width="0.53" height="22.9"></rect>
		<polygon points="89.71 105.17 87.46 104.16 89.71 108.62 91.95 104.16 89.71 105.17"></polygon>
	</g>
	<g class="snowflake-dec-arrow">
		<rect x="89.45" y="143.46" width="0.53" height="12.96"></rect>
		<polygon points="89.72 154.98 87.46 153.96 89.71 158.42 91.95 153.96 89.72 154.98"></polygon>
	</g>
	<g class="snowflake-dec-arrow">
		<rect x="89.44" y="193.26" width="0.53" height="12.96"></rect>
		<polygon points="89.7 204.78 87.45 203.77 89.7 208.22 91.94 203.77 89.7 204.78"></polygon>
	</g>
	<text class="snowflake-dec-number snowflake-dec-sz-normal" transform="translate(51.34 129.68)"> 41944705796 </text>
	<text class="snowflake-dec-number snowflake-dec-sz-normal" transform="translate(44.51 179.5)"> 1462015105796 </text>
	<text class="snowflake-dec-number snowflake-dec-sz-small" transform="translate(445.78 56.28)"> 0 </text>
	<text class="snowflake-dec-number snowflake-dec-sz-small" transform="translate(363.84 56.28)"> 12 </text>
	<text class="snowflake-dec-number snowflake-dec-sz-small" transform="translate(323.71 56.28)"> 17 </text>
	<text class="snowflake-dec-number snowflake-dec-sz-small" transform="translate(290.96 56.28)"> 22 </text>
	<text class="snowflake-dec-number snowflake-dec-sz-small" transform="translate(0 56.37)"> 64 </text>
	<text class="snowflake-dec-text snowflake-dec-sz-small" transform="translate(132.54 147.05)">
		+ 1420070400000
		<tspan x="-34.99" y="9.6"> Discord Epoch (unix timestamp in ms) </tspan>
	</text>
	<text class="snowflake-dec-text snowflake-dec-sz-small" transform="translate(96.64 200.66)"> Parse unix timstamp (ms) </text>
	<text class="snowflake-dec-text snowflake-dec-sz-normal" transform="translate(8.05 229.66)"> 2016-04-30 11:18:25.796 UTC </text>
	<text class="snowflake-dec-text snowflake-dec-sz-small" transform="translate(93.2 96.22)"> to decimal </text>
</svg>

<style>
	@font-face {
		font-family: "Source Code Pro";
		font-style: normal;
		font-weight: 400;
		font-display: swap;
		src: url(/fonts/sourcecodepro.woff2) format("woff2");
		unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+0304, U+0308, U+0329, U+2000-206F, U+2074, U+20AC, U+2122,
			U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
	}

	.snowflake-dec-sz-normal {
		font-size: 12px;
	}

	.snowflake-dec-sz-small {
		font-size: 8px;
	}

	.snowflake-dec-number {
		font-family:
			Source Code Pro,
			monospace;
	}

	.snowflake-dec-text {
		font-family: ArialMT, Arial, sans-serif;
	}

	.snowflake-dec-reduce-space {
		letter-spacing: -0.02em;
	}

	.snowflake-dec-ms {
		fill: #3cb119;
	}

	[data-theme="dark"] .snowflake-dec-ms {
		fill: #7cf559;
	}

	.snowflake-dec-worker {
		fill: #7d24d6;
	}

	[data-theme="dark"] .snowflake-dec-worker {
		fill: #a65eef;
	}

	.snowflake-dec-process {
		fill: #de0ede;
	}

	[data-theme="dark"] .snowflake-dec-process {
		fill: #ec40c4;
	}

	.snowflake-dec-increment {
		fill: #e40606;
	}

	[data-theme="dark"] .snowflake-dec-increment {
		fill: #f75454;
	}
</style>
